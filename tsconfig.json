{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "jsx": "react",
      "lib": ["es6", "dom"],
      "strict": true,
      "resolveJsonModule": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": ".",
      "baseUrl": "."
    },
    "exclude": ["node_modules", "vendor"]
  }